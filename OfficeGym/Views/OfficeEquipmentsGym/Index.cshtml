@model IEnumerable<OfficeGym.Models.OfficeEquipmentGym>

@{
    ViewData["Title"] = "Office equipments gym";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Office Equipments Gym</h1>

<p>
    <a asp-action="Create">Create new</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EquipmentGymName)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EquipmentGymName)
            </td>
        </tr>
        }
    </tbody>
</table>